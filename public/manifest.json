{
  "name": "SoundFlow",
  "short_name": "SoundFlow",
  "description": "Stream your music with SoundFlow",
  "theme_color": "#0d1117",
  "background_color": "#0d1117",
  "display": "minimal-ui",
  "orientation": "any",
  "scope": "/",
  "start_url": "/",
  "id": "/",
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "url": "shared_url"
    }
  }
}